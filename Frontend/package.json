{
  "name": "Frontend",
  "version": "0.0.1",
  "main": "electron.js",
  "author": "Ionic Framework",
  "homepage": "https://ionicframework.com/",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "ng lint",
    "electron": "electron .",
    "electron:dev": "concurrently \"ng serve\" \"wait-on http://localhost:8100 && electron .\"",
    "electron:build": "ng build --configuration production && electron-builder build --win",
    "electron:build:all": "ng build --configuration production && electron-builder build --win --x64 --ia32"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "18.2.14",
    "@angular/common": "18.2.14",
    "@angular/compiler": "18.2.14",
    "@angular/core": "18.2.14",
    "@angular/forms": "18.2.14",
    "@angular/platform-browser": "18.2.14",
    "@angular/platform-browser-dynamic": "18.2.14",
    "@angular/router": "18.2.14",
    "@auth0/angular-jwt": "^5.2.0",
    "@capacitor/android": "7.0.0",
    "@capacitor/app": "7.0.0",
    "@capacitor/core": "7.0.0",
    "@capacitor/haptics": "7.0.0",
    "@capacitor/keyboard": "7.0.0",
    "@capacitor/status-bar": "7.0.0",
    "@ionic/angular": "8.7.8",
    "ionicons": "^8.0.13",
    "moment": "^2.29.4",
    "ngx-socket-io": "^4.9.3",
    "rxjs": "7.8.2",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.10"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.2.21",
    "@angular-eslint/builder": "^20.0.0",
    "@angular-eslint/eslint-plugin": "^20.0.0",
    "@angular-eslint/eslint-plugin-template": "^20.0.0",
    "@angular-eslint/schematics": "^20.0.0",
    "@angular-eslint/template-parser": "^20.0.0",
    "@angular/cli": "18.2.21",
    "@angular/compiler-cli": "^18.2.14",
    "@angular/language-service": "^20.0.0",
    "@capacitor/cli": "7.4.4",
    "@ionic/angular-toolkit": "^12.0.0",
    "@types/jasmine": "~5.1.0",
    "@types/jasminewd2": "^2.0.13",
    "@types/jest": "^30.0.0",
    "@types/json-schema": "^7.0.15",
    "@types/node": "^24.9.2",
    "@types/socket.io-client": "^1.4.36",
    "@typescript-eslint/eslint-plugin": "^8.18.0",
    "@typescript-eslint/parser": "^8.18.0",
    "concurrently": "^9.2.1",
    "electron": "^39.2.0",
    "electron-builder": "^26.0.12",
    "eslint": "^9.16.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsdoc": "^48.2.1",
    "eslint-plugin-prefer-arrow": "1.2.2",
    "jasmine-core": "~5.1.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.5.0",
    "wait-on": "^9.0.3"
  },
  "description": "An Ionic project",
  "build": {
    "appId": "de.bso.feueralarm",
    "productName": "Feueralarm BSO",
    "directories": {
      "output": "dist-electron",
      "buildResources": "build"
    },
    "files": [
      "www/**/*",
      "electron.js"
    ],
    "win": {
      "target": [
        "portable",
        "nsis"
      ],
      "icon": "src/assets/icon/icon.png"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true
    },
    "portable": {
      "artifactName": "Feueralarm-BSO-Portable.exe"
    }
  }
}
